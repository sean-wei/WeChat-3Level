.hotProduct:before {
  content: '';
  display: block;
  width: 46px;
  height: 1px;
  background: #fff;
  position: absolute;
  left: 10px;
  top: 50%;
  transition: ease 0.65s;
}

.hotProduct:after {
  content: '';
  display: block;
  width: 46px;
  height: 1px;
  background: #fff;
  position: absolute;
  right: 10px;
  top: 50%;
  transition: ease 0.65s;
}

